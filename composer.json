{
    "name": "outerweb/file-integrity",
    "description": "A package that aids in checking the integrity of files on your server",
    "keywords": [
        "outerweb",
        "laravel",
        "file-integrity"
    ],
    "homepage": "https://github.com/outer-web/file-integrity",
    "license": "MIT",
    "authors": [
        {
            "name": "Outerweb",
            "email": "info@outerweb.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^11.0||^12.0",
        "illuminate/support": "^11.0||^12.0",
        "illuminate/container": "^11.0||^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.8"
    },
    "autoload": {
        "psr-4": {
            "Outerweb\\FileIntegrity\\": "src/"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Outerweb\\FileIntegrity\\FileIntegrityServiceProvider"
            ],
            "aliases": {
                "FileIntegrity": "Outerweb\\FileIntegrity\\Facades\\FileIntegrity"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
